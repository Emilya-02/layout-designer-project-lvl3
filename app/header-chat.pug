.header.bg-primary.d-flex.justify-content-between.align-items-center.pt-3.pb-3
  a.ms-4.mx-auto(href='#')
    img.color-fill(src='images/icons/logo.svg', width='15', alt='главная')
  nav.d-flex.nav#mytab(role='tablist')
    a.d-flex.mx-3.align-items-center.text-decoration-none.sms-icon#current-chat-tab(href='#current-chat', data-bs-toggle='tab', role='tab', aria-controls='current-chat', aria-selected='true')
      img.color-fill.me-2(src='images/icons/chat-left-text 1.svg', width='20', height='40', alt='мессенджер')
    a.d-flex.mx-3.align-items-center.text-decoration-none#contacts-tab(href='#contacts', data-bs-toggle='tab', role='tab', aria-controls='contacts', aria-selected='true')
      img.color-fill.me-2(src='images/icons/people.svg', width='20', height='40', alt='контакты')
      span.text-light Контакты
    a.d-flex.mx-3.align-items-center.text-decoration-none#chats-tab(href='#chats', data-bs-toggle='tab', role='tab', aria-controls='chats', aria-selected='true')
      img.color-fill.me-2(src='images/icons/chat.svg', width='20', height='40', alt='сообщения')
      span.text-light Сообщения
    a.d-flex.mx-3.align-items-center.text-decoration-none#profile-tab(href='#profile', data-bs-toggle='tab', role='tab', aria-controls='profile', aria-selected='true')
      img.color-fill.me-2(src='images/icons/person.svg', width='20', height='40', alt='профиль')
      span.text-light Профиль